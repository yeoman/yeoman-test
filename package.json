    "unit test"
  ],
  "repository": "yeoman/yeoman-test",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^3.0.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-xo": "^0.29.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "tui-jsdoc-template": "^1.2.2"
  },
  "dependencies": {
    "inquirer": "^7.1.0",
    "lodash": "^4.17.15",
    "mem-fs": "^1.2.0",
    "mem-fs-editor": "^7.0.1",
    "mkdirp": "^1.0.3",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.1",
    "yeoman-environment": "^2.10.0",
    "yeoman-generator": "^4.10.0"
  },
  "scripts": {
    "test": "nyc mocha",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
